		"vscode": "0.10.x"
	},
	"activationEvents": [
		"onLanguage:javascript",
		"onLanguage:javascriptreact"
	],
	"main": "./out/extension",
	"contributes": {
		"configuration": {
			"type": "object",
			"title": "ESLint configuration",
			"properties": {
				"eslint.enable": {
					"type": "boolean",
					"default": true,
					"description": "Control whether eslint is enabled for JavaScript files or not."
				},
				"eslint.options": {
					"type": "object",
					"default": {},
					"description": "The eslint options object to provide args to the eslint command."
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "cd ../eslint-server && npm run compile && cd ../eslint && tsc -p ./",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./"
	},
	"devDependencies": {
		"vscode": "0.10.x",
		"typescript": "^1.6.2"
	},
	"dependencies": {
		"vscode-languageclient": "0.10.x"
	}
